{
  "name": "backend",
  "version": "1.0.0",
  "description": "backend solution for this project",
  "main": "./src/index.js",
  "scripts": {
    "set-node-env-dev": "export NODE_ENV=Development",
    "set-node-env-test": "export NODE_ENV=test",
    "test": "yarn run set-node-env-test && jest --verbose --silent",
    "db:create:test": "cross-env NODE_ENV=test npx sequelize-cli db:create",
    "db:migrate:test": "cross-env NODE_ENV=test npx sequelize db:migrate",
    "devStart": "yarn run set-node-env-dev && nodemon",
    "testStart": "yarn run set-node-env-test && nodemon "
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "author": "Lucas Lucena",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "pg": "^8.3.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.3.4",
    "sequelize-cli": "^6.2.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "eslint": "^7.7.0",
    "jest": "^26.4.0",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2"
  }
}
